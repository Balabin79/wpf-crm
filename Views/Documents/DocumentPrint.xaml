<Window x:Class="Dental.Views.Documents.DocumentPrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dental.Views.Documents"
        xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing" 
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        mc:Ignorable="d"
        xmlns:services="clr-namespace:Dental.Services"
        Icon="{x:Static services:ParamsApp.Icon}" 
        Title="{x:Static services:ParamsApp.Title}" 
        Height="450" Width="800">
    <Grid>
        <dx:LoadingDecorator SplashScreenDataContext="Загрузка документа" >
            <dxp:DocumentPreviewControl x:Name="doc"/>
        </dx:LoadingDecorator>
    </Grid>
</Window>
